{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/public/static/**",
      "use": "@vercel/static"
    },
    {
      "src": "client/public/figmaAssets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/client/public/static/$1"
    },
    {
      "src": "/figmaAssets/(.*)",
      "dest": "/client/public/figmaAssets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
